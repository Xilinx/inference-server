/**
 * @file
 * @brief Defines the Proteus's version information
 */

#ifndef GUARD_PROTEUS_VERSION_HPP
#define GUARD_PROTEUS_VERSION_HPP

/// Proteus's full version as a string
constexpr auto kProteusVersion = "${ver}";

/// Proteus's major version as an integer
constexpr auto kProteusVersionMajor = ${ver_major};
/// Proteus's minor version as an integer
constexpr auto kProteusVersionMinor = ${ver_minor};
/// Proteus's patch version as an integer
constexpr auto kProteusVersionPatch = ${ver_patch};
/// Proteus's version label as a string
constexpr auto kProteusVersionLabel = "${ver_label}";
#endif  // GUARD_PROTEUS_VERSION_HPP
