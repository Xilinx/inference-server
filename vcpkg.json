{
  "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg-tool/main/docs/vcpkg.schema.json",
  "name": "amdinfer",
  "version-semver": "0.4.0",
  "dependencies": [
    {
      "name": "b64",
      "version>=": "2.0.0.1"
    },
    {
      "name": "benchmark",
      "version>=": "1.7.1"
    },
    "boost-process",
    {
      "name": "concurrentqueue",
      "version>=": "1.0.3"
    },
    {
      "name": "cxxopts",
      "version>=": "2.2.1"
    },
    {
      "name": "drogon",
      "version>=": "1.8.1"
    },
    {
      "name": "efsw",
      "version>=": "1.3.1#1"
    },
    {
      "name": "half",
      "version>=": "2.2.0"
    },
    {
      "name": "grpc",
      "version>=": "1.44.0",
      "features": [
        "codegen"
      ]
    },
    {
      "name": "gtest",
      "version>=": "1.11.0"
    },
    {
      "name": "opentelemetry-cpp",
      "version>=": "1.8.3",
      "features": [
        "otlp",
        "otlp-http"
      ]
    },
    {
      "name": "prometheus-cpp",
      "version>=": "1.1.0"
    },
    {
      "name": "protobuf",
      "version>=": "3.19.4"
    },
    {
      "name": "opencv4",
      "default-features": false,
      "features": [
        "ffmpeg",
        "jpeg"
      ]
    },
    {
      "name": "spdlog",
      "version>=": "1.8.5"
    },
    {
      "name": "tomlplusplus",
      "version>=": "3.1.0"
    },
    {
      "name": "unilog",
      "version>=": "3.0.0"
    },
    {
      "name": "vcpkg-cmake",
      "host": true
    },
    {
      "name": "vcpkg-cmake-config",
      "host": true
    }
  ],
  "overrides": [
    {
      "name": "cxxopts",
      "version": "2.2.1"
    },
    {
      "name": "opentelemetry-cpp",
      "version": "1.8.3#7"
    }
  ],
  "vcpkg-configuration": {
    "default-registry": {
      "kind": "git",
      "repository": "https://github.com/microsoft/vcpkg",
      "baseline": "501db0f17ef6df184fcdbfbe0f87cde2313b6ab1"
    },
    "overlay-ports": [
      "./external/vcpkg/ports"
    ],
    "overlay-triplets": [
      "./external/vcpkg/triplets"
    ]
  }
}
